{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit: Run Current File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/streamlit",
            "args": [
                "--log_level",
                "debug",
                "run",
                "${workspaceFolder}/main.py",
            ],
            "console": "integratedTerminal"
        }
    ]
}